{
  "name": "pika-wildfires",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve-data": "cd data && http-server . --cors --port 8080",
    "dev": "concurrently --kill-others \"npm run serve-data\" \"vite dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "concurrently": "^8.0.1",
    "http-server": "^14.1.1",
    "maplibre-gl": "^3.1.0",
    "pmtiles": "^2.7.2",
    "protomaps-themes-base": "^1.3.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  },
  "type": "module"
}
